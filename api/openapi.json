  "components": {
    "schemas": {
      "ConfigModel": {
        "type": "object",
        "properties": {
          "defaultMapBounds": {
            "$ref": "#/components/schemas/MapBounds"
          },
          "municipalityName": {
            "type": "string"
          }
        }
      },
      "LocationModel": {
        "required": ["lat", "lng"],
        "type": "object",
        "properties": {
          "lat": {
            "type": "number",
            "format": "double"
          },
          "lng": {
            "type": "number",
            "format": "double"
          }
        }
      },
      "MapBounds": {
        "type": "object",
        "properties": {
          "corner1": {
            "$ref": "#/components/schemas/LocationModel"
          },
          "corner2": {
            "$ref": "#/components/schemas/LocationModel"
          }
        }
      },
      "AmountModel": {
        "required": ["amount", "id"],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "amount": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "GroundPosition": {
        "type": "object",
        "properties": {
          "noPermissionOwner": {
            "type": "integer",
            "format": "int32"
          },
          "intentionPermissionOwner": {
            "type": "integer",
            "format": "int32"
          },
          "formalPermissionOwner": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "HouseType": {
        "type": "object",
        "properties": {
          "meergezinswoning": {
            "type": "integer",
            "format": "int32"
          },
          "eengezinswoning": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "HouseblockSnapshotModel": {
        "type": "object",
        "properties": {
          "startDate": {
            "type": "string",
            "format": "date"
          },
          "endDate": {
            "type": "string",
            "format": "date"
          },
          "projectId": {
            "type": "string",
            "format": "uuid"
          },
          "houseblockId": {
            "type": "string",
            "format": "uuid"
          },
          "houseblockName": {
            "type": "string"
          },
          "size": {
            "$ref": "#/components/schemas/SingleValueOrRangeModelBigDecimal"
          },
          "programming": {
            "type": "boolean"
          },
          "mutation": {
            "$ref": "#/components/schemas/Mutation"
          },
          "ownershipValue": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OwnershipValue"
            }
          },
          "groundPosition": {
            "$ref": "#/components/schemas/GroundPosition"
          },
          "physicalAppearance": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AmountModel"
            }
          },
          "houseType": {
            "$ref": "#/components/schemas/HouseType"
          },
          "targetGroup": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AmountModel"
            }
          },
          "customProperties": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ProjectHouseblockCustomPropertyModel"
            }
          }
        }
      },
      "Mutation": {
        "type": "object",
        "properties": {
          "kind": {
            "type": "string",
            "enum": ["CONSTRUCTION", "DEMOLITION"]
          },
          "amount": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "OwnershipValue": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "type": {
            "type": "string",
            "enum": ["KOOPWONING", "HUURWONING_PARTICULIERE_VERHUURDER", "HUURWONING_WONINGCORPORATIE"]
          },
          "amount": {
            "type": "integer",
