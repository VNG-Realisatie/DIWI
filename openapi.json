  "components": {
    "schemas": {
      "SelectModel": {
        "required": ["id", "name"],
        "type": "object",
        "properties": { "id": { "type": "string", "format": "uuid" }, "name": { "type": "string" } }
      },
      "CustomPropertyModel": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "format": "uuid" },
          "name": { "type": "string" },
          "objectType": { "type": "string", "enum": ["PROJECT", "WONINGBLOK"] },
          "propertyType": { "type": "string", "enum": ["BOOLEAN", "CATEGORY", "ORDINAL", "NUMERIC", "TEXT"] },
          "disabled": { "type": "boolean" },
          "categories": { "type": "array", "items": { "$ref": "#/components/schemas/SelectModel" } }
        }
      },
      "GroundPosition": {
        "type": "object",
        "properties": {
          "noPermissionOwner": { "type": "integer", "format": "int32" },
          "intentionPermissionOwner": { "type": "integer", "format": "int32" },
          "formalPermissionOwner": { "type": "integer", "format": "int32" }
        }
      },
      "HouseType": {
        "type": "object",
        "properties": { "meergezinswoning": { "type": "integer", "format": "int32" }, "eengezinswoning": { "type": "integer", "format": "int32" } }
      },
      "HouseblockSnapshotModel": {
        "type": "object",
        "properties": {
          "startDate": { "type": "string", "format": "date" },
          "endDate": { "type": "string", "format": "date" },
          "projectId": { "type": "string", "format": "uuid" },
          "houseblockId": { "type": "string", "format": "uuid" },
          "houseblockName": { "type": "string" },
          "size": { "$ref": "#/components/schemas/SingleValueOrRangeModelBigDecimal" },
          "programming": { "type": "boolean" },
          "mutation": { "$ref": "#/components/schemas/Mutation" },
          "ownershipValue": { "type": "array", "items": { "$ref": "#/components/schemas/OwnershipValue" } },
          "groundPosition": { "$ref": "#/components/schemas/GroundPosition" },
          "physicalAppearance": { "$ref": "#/components/schemas/PhysicalAppearance" },
          "houseType": { "$ref": "#/components/schemas/HouseType" },
          "purpose": { "$ref": "#/components/schemas/Purpose" }
        }
      },
      "Mutation": {
        "type": "object",
        "properties": {
          "mutationKind": { "type": "array", "items": { "type": "string", "enum": ["BOUW", "SLOOP", "TRANSFORMATIE", "SPLITSING"] } },
          "grossPlanCapacity": { "type": "integer", "format": "int32" },
          "netPlanCapacity": { "type": "integer", "format": "int32" },
          "demolition": { "type": "integer", "format": "int32" }
        }
      },
      "OwnershipValue": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "format": "uuid" },
          "type": { "type": "string", "enum": ["KOOPWONING", "HUURWONING_PARTICULIERE_VERHUURDER", "HUURWONING_WONINGCORPORATIE"] },
          "amount": { "type": "integer", "format": "int32" },
          "value": { "$ref": "#/components/schemas/SingleValueOrRangeModelInteger" },
          "rentalValue": { "$ref": "#/components/schemas/SingleValueOrRangeModelInteger" }
        }
      },
      "PhysicalAppearance": {
        "type": "object",
        "properties": {
          "tussenwoning": { "type": "integer", "format": "int32" },
          "tweeondereenkap": { "type": "integer", "format": "int32" },
          "portiekflat": { "type": "integer", "format": "int32" },
          "hoekwoning": { "type": "integer", "format": "int32" },
          "vrijstaand": { "type": "integer", "format": "int32" },
          "gallerijflat": { "type": "integer", "format": "int32" }
        }
      },
      "Purpose": {
        "type": "object",
        "properties": {
          "regular": { "type": "integer", "format": "int32" },
          "youth": { "type": "integer", "format": "int32" },
          "student": { "type": "integer", "format": "int32" },
          "elderly": { "type": "integer", "format": "int32" },
          "largeFamilies": { "type": "integer", "format": "int32" },
          "ghz": { "type": "integer", "format": "int32" }
        }
      },
      "SingleValueOrRangeModelBigDecimal": {
        "type": "object",
        "properties": { "value": { "type": "number" }, "min": { "type": "number" }, "max": { "type": "number" } }
      },
      "SingleValueOrRangeModelInteger": {
        "type": "object",
        "properties": {
          "value": { "type": "integer", "format": "int32" },
          "min": { "type": "integer", "format": "int32" },
          "max": { "type": "integer", "format": "int32" }
        }
      },
      "MilestoneModel": {
        "required": ["id", "stateId", "status"],
        "type": "object",
        "properties": {
          "id": { "type": "string", "format": "uuid" },
          "stateId": { "type": "string", "format": "uuid" },
          "status": { "type": "string", "enum": ["VOORSPELD", "GEPLAND", "GEREALISEERD", "AFGEBROKEN"] },
          "date": { "type": "string", "format": "date" },
          "description": { "type": "string" }
        }
      },
      "OrganizationModel": {
        "required": ["name", "uuid"],
        "type": "object",
        "properties": {
          "uuid": { "type": "string", "format": "uuid" },
          "name": { "type": "string" },
          "users": { "type": "array", "items": { "$ref": "#/components/schemas/OrganizationUserModel" } }
        }
      },
      "OrganizationUserModel": {
        "type": "object",
        "properties": {
          "uuid": { "type": "string", "format": "uuid" },
          "firstName": { "type": "string" },
          "lastName": { "type": "string" },
          "initials": { "type": "string" }
        }
      },
      "ProjectMinimalSnapshotModel": {
        "required": ["confidentialityLevel", "projectColor", "projectId", "projectName", "projectPhase"],
        "type": "object",
        "properties": {
          "startDate": { "type": "string", "format": "date" },
          "endDate": { "type": "string", "format": "date" },
          "projectName": { "type": "string" },
          "projectColor": { "type": "string" },
          "confidentialityLevel": { "type": "string", "enum": ["PRIVE", "INTERN_UITVOERING", "INTERN_RAPPORTAGE", "EXTERN_RAPPORTAGE", "OPENBAAR"] },
          "projectPhase": {
            "type": "string",
            "enum": ["_1_INITIATIEFFASE", "_2_PROJECTFASE", "_3_VERGUNNINGSFASE", "_4_REALISATIEFASE", "_5_OPLEVERINGSFASE"]
          },
          "projectId": { "type": "string", "format": "uuid" }
        }
      },
      "ProjectCreateSnapshotModel": {
        "required": ["confidentialityLevel", "projectColor", "projectName", "projectPhase"],
        "type": "object",
        "properties": {
          "startDate": { "type": "string", "format": "date" },
          "endDate": { "type": "string", "format": "date" },
          "projectName": { "type": "string" },
          "projectColor": { "type": "string" },
          "confidentialityLevel": { "type": "string", "enum": ["PRIVE", "INTERN_UITVOERING", "INTERN_RAPPORTAGE", "EXTERN_RAPPORTAGE", "OPENBAAR"] },
          "projectPhase": { "type": "string", "enum": ["_1_INITIATIEFFASE", "_2_PROJECTFASE", "_3_VERGUNNINGSFASE", "_4_REALISATIEFASE", "_5_OPLEVERINGSFASE"] }
        }
      },
      "PriorityModel": {
        "type": "object",
        "properties": {
          "value": { "$ref": "#/components/schemas/SelectModel" },
          "min": { "$ref": "#/components/schemas/SelectModel" },
          "max": { "$ref": "#/components/schemas/SelectModel" }
        }
      },
      "ProjectListModel": {
        "required": ["confidentialityLevel", "projectColor", "projectId", "projectName", "projectPhase"],
        "type": "object",
        "properties": {
          "startDate": { "type": "string", "format": "date" },
          "endDate": { "type": "string", "format": "date" },
          "projectName": { "type": "string" },
          "projectColor": { "type": "string" },
          "confidentialityLevel": { "type": "string", "enum": ["PRIVE", "INTERN_UITVOERING", "INTERN_RAPPORTAGE", "EXTERN_RAPPORTAGE", "OPENBAAR"] },
          "projectPhase": {
            "type": "string",
            "enum": ["_1_INITIATIEFFASE", "_2_PROJECTFASE", "_3_VERGUNNINGSFASE", "_4_REALISATIEFASE", "_5_OPLEVERINGSFASE"]
          },
          "projectId": { "type": "string", "format": "uuid" },
          "projectStateId": { "type": "string", "format": "uuid" },
          "planningPlanStatus": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": [
                "_1A_ONHERROEPELIJK",
                "_1B_ONHERROEPELIJK_MET_UITWERKING_NODIG",
                "_1C_ONHERROEPELIJK_MET_BW_NODIG",
                "_2A_VASTGESTELD",
                "_2B_VASTGESTELD_MET_UITWERKING_NODIG",
                "_2C_VASTGESTELD_MET_BW_NODIG",
                "_3_IN_VOORBEREIDING",
                "_4A_OPGENOMEN_IN_VISIE",
                "_4B_NIET_OPGENOMEN_IN_VISIE"
              ]
            }
          },
          "planType": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": ["PAND_TRANSFORMATIE", "TRANSFORMATIEGEBIED", "HERSTRUCTURERING", "VERDICHTING", "UITBREIDING_UITLEG", "UITBREIDING_OVERIG"]
