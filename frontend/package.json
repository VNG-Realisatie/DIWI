{
  "name": "frontend",
  "version": "1.2.0-rc",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.4",
    "@mui/x-data-grid": "^7.1.0",
    "@mui/x-date-pickers": "^7.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/autolinker": "^2.0.0",
    "@types/jest": "^27.0.1",
    "@types/leaflet-draw": "^1.0.11",
    "@types/lodash": "^4.17.0",
    "@types/node": "^16.7.13",
    "@types/ol": "^7.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autolinker": "^4.0.0",
    "d3": "^7.8.5",
    "dayjs": "^1.11.9",
    "i18next": "^23.8.1",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "lodash": "^4.17.21",
    "ol": "^9.0.0",
    "path-to-regexp": "^6.2.1",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "reproject": "^1.2.7",
    "sass": "^1.65.1",
    "source-map-loader": "^5.0.0",
    "svg": "^0.1.0",
    "swagger-ui-react": "^4.12.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": ". ../version.sh && GENERATE_SOURCEMAP=false react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "yarn test --testResultsProcessor=\"jest-junit\" --watchAll=false --ci --coverage",
    "eject": "react-scripts eject",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "lint": "eslint --max-warnings 0 src/",
    "lint:ci": "eslint --max-warnings 0 --format junit --output-file eslint.xml src/",
    "create-types": "openapi-typescript ../api/openapi.json -o src/types/schema.d.ts && prettier --write ../api/openapi.json src/types/schema.d.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/leaflet": "^1.9.8",
    "@types/react-color": "^3.0.6",
    "@types/swagger-ui-react": "^4.18.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest-junit": "^16.0.0",
    "openapi-typescript": "^6.7.4",
    "prettier": "^3.2.5"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-leaflet|leaflet|leaflet-draw|swagger-ui-react|swagger-client|react-syntax-highlighter|query-string|decode-uri-component|split-on-first|filter-obj|ol|color-space|color-rgba|color-parse|color-name|quick-lru)/)"
    ],
    "moduleNameMapper": {
      "^\\@swagger-api/apidom-reference": "<rootDir>/node_modules/@swagger-api/apidom-reference/dist/apidom-reference.browser.min.js",
      "d3": "<rootDir>/node_modules/d3/dist/d3.min.js",
      "^d3-(.*)$": "<rootDir>/node_modules/d3-$1/dist/d3-$1.min.js"
    }
  }
}
