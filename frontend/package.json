    "@mui/x-date-pickers": "^6.11.2",
    "@nivo/bar": "^0.83.0",
    "@nivo/pie": "^0.83.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/leaflet-draw": "^1.0.11",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "dayjs": "^1.11.9",
    "i18next": "^23.8.1",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "path-to-regexp": "^6.2.1",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "sass": "^1.65.1",
    "swagger-ui-react": "^5.11.4",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "yarn test --testResultsProcessor=\"jest-junit\" --watchAll=false --ci --coverage",
    "eject": "react-scripts eject",
    "format": "prettier --fix --write src/",
    "format:check": "prettier --check src/",
    "lint": "eslint --max-warnings 0 src/",
    "lint:ci": "eslint --max-warnings 0 --format junit --output-file eslint.xml src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/react-color": "^3.0.6",
    "@types/swagger-ui-react": "^4.18.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest-junit": "^16.0.0",
    "prettier": "^3.2.5"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!(@react-leaflet|react-leaflet|leaflet|leaflet-draw)/)"
    ]
  }
}
